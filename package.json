{
  "name": "react-slick",
  "version": "0.14.5",
  "description": " React port of slick carousel",
  "main": "./lib",
  "scripts": {
    "watch-start": "watch-run -p 'src/**' npm run build",
    "start": "gulp server",
    "clean": "rm -rf lib/",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run clean && npm run build && gulp dist",
    "preversion": "rm -rf lib/ && ./node_modules/.bin/babel src --out-dir lib --stage 0 --optional runtime  && git add --all lib/ && git commit --allow-empty -m \"Add new lib version\"",
    "postversion": "git push origin master && git push origin --tags",
    "test": "jest",
    "dev-test": "jest --watch"
  },
  "author": "Kiran Abburi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akiran/react-slick"
  },
  "keywords": [
    "slick",
    "carousel",
    "Image slider",
    "orbit",
    "slider",
    "react-component"
  ],
  "devDependencies": {
    "babel-cli": "6.7.7",
    "babel-core": "6.7.7",
    "babel-eslint": "6.1.0",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.9.0",
    "css-loader": "^0.23.1",
    "deepmerge": "^0.2.10",
    "del": "^2.2.0",
    "enzyme": "^2.3.0",
    "es5-shim": "^4.5.8",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "foundation-apps": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "run-sequence": "^1.2.1",
    "sass-loader": "^3.2.0",
    "should": "^8.4.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "watch-run": "^1.2.5",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "autoprefixer-core": "^6.0.1",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-airbnb": "^2.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "json2mq": "^0.2.0",
    "react-responsive-mixin": "^0.3.5",
    "slick-carousel": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.1",
    "react-dom": "^0.14.0 || ^15.0.1"
  },
  "jest": {
    "setupFiles": ["test-setup.js"]
  },
  "npmName": "react-slick",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "bugs": {
    "url": "https://github.com/akiran/react-slick/issues"
  },
  "homepage": "https://github.com/akiran/react-slick"
}
